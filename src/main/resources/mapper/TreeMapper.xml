<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhaozy.test.dao.StudentDao">
    <select id="selectAll" resultType="com.zhaozy.test.entity.Student" useCache="true">
        select * from student
    </select>
    <select id="selectById" resultType="com.zhaozy.test.entity.Student" useCache="true">
        select * from student where id = #{id}
    </select>
</mapper>